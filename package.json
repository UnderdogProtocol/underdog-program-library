{
  "name": "@underdog-protocol/underdog-program-library",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "turbo run clean; rm -rf node_modules yarn.lock",
    "build": "turbo run build",
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
    "clippy": "cargo clippy --all-targets -- -D warnings -A clippy::result_large_err -A clippy::too_many_arguments",
    "underdog-core-sdk": "yarn workspace @underdog-protocol/underdog-core-sdk",
    "generate:id": "yarn workspace @underdog-protocol/underdog-identity-sdk generate",
    "generate": "yarn generate:idls && yarn generate:clients",
    "generate:clients": "node ./configs/kinobi.js",
    "generate:idls": "node ./shank.js"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.26.0",
    "@coral-xyz/anchor-cli": "^0.26.0",
    "@coral-xyz/borsh": "^0.2.6",
    "@metaplex-foundation/umi": "^0.8.7",
    "@metaplex-foundation/umi-bundle-defaults": "^0.8.7",
    "@solana/web3.js": "^1.78.4",
    "turbo": "^1.10.12"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@metaplex-foundation/kinobi": "^0.12.3",
    "@metaplex-foundation/shank-js": "^0.1.5",
    "@types/bn.js": "^5.1.0",
    "@types/jest": "^29.5.3",
    "jest": "^29.6.2",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0",
    "typescript": "^4.3.5"
  },
  "packageManager": "yarn@1.22.17"
}
